{
  "mimeTypes": {
    ".js": "application/javascript",
    ".mjs": "application/javascript",
    ".css": "text/css",
    ".svg": "image/svg+xml",
    ".json": "application/json",
    ".wasm": "application/wasm",
    ".mp3": "audio/mpeg",
    ".ogg": "audio/ogg",
    ".wav": "audio/wav",
    ".m4a": "audio/mp4",
    ".webm": "audio/webm"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/*.js",
      "/*.css",
      "/*.map",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/*.ico",
      "/*.webp",
      "/*.woff",
      "/*.woff2",
      "/__build_id.txt",
      "/sounds/*"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated", "anonymous"],
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "rewrite": "/api/{*path}"
    },
    {
      "route": "/xapi",
      "redirect": "/xapi/",
      "statusCode": 301
    },
    {
      "route": "/xapi/*",
      "allowedRoles": ["anonymous"],
      "rewrite": "/api/{*path}"
    },
    {
      "route": "/admin",
      "allowedRoles": ["anonymous"],
      "headers": { "X-Robots-Tag": "noindex, nofollow" }
    },
    {
      "route": "/admin/*",
      "allowedRoles": ["anonymous"],
      "headers": { "X-Robots-Tag": "noindex, nofollow" }
    },
    {
      "route": "/bulk-import",
      "allowedRoles": ["authenticated"],
      "headers": { "X-Robots-Tag": "noindex, nofollow" }
    },
    {
      "route": "/login",
      "headers": { "X-Robots-Tag": "noindex, nofollow" }
    }
  ],
  "platform": {
    "apiRuntime": "node:20"
  }
}
